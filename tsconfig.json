{
	"compilerOptions": {
		"lib": [
			"ES2023"
		],
		"incremental": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"outDir": "./out",
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noPropertyAccessFromIndexSignature": true,
		"noUncheckedIndexedAccess": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"strict": true,
		"target": "ES2023",
	},
	"include": [
		"src/**/*.ts"
	]
}
